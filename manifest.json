{
  "manifest_version": 3,
  "name": "Prompt Index Sidebar",
  "version": "0.1.0",
  "description": "Index and jump to your previous prompts in AI chats.",
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*"
      ],
      "js": ["content.js"],
      "css": ["sidebar.css"]
    }
  ]
}
